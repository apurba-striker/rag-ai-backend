{
  "name": "rag-news-chatbot-backend",
  "version": "1.0.0",
  "description": "RAG-powered news chatbot backend with Neon PostgreSQL",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "ingest": "node scripts/ingestNews.js",
    "ingest:large": "node scripts/ingestNews.js 100",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "test": "jest",
    "docker:build": "docker build -t rag-chatbot-backend .",
    "docker:run": "docker run -p 5000:5000 rag-chatbot-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1",
    "@google/generative-ai": "^0.2.1",
    "@qdrant/js-client-rest": "^1.4.0",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "rss-parser": "^3.13.0",
    "uuid": "^9.0.0",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.8.1",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "sequelize-cli": "^6.6.1"
  }
}
